{
    "providerId": "goshopmatic.com",
    "providerName": "Shopmatic",
    "serviceId": "website",
    "serviceName": "Shopmatic Site",
    "logoUrl": "/modules/domain-connect/logos/goshopmatic.com.website.png",
    "description": "Enables a domain to work with Shopmatic",
    "variableDescription": "INPUTDOMAIN is domain name being configured to work with shopmatic.",
    "syncBlock": false,
    "shared": true,
    "warnPhishing": true,
    "hostRequired": false,
    "version": 1,
    "records": [
        {
            "type": "CNAME",
            "host": "www",
            "pointsTo": "%INPUTDOMAIN%",
            "ttl": "3600"
        }
    ]
}
