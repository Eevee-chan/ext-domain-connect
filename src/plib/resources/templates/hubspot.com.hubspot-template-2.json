{
    "providerId": "hubspot.com",
    "providerName": "HubSpot",
    "serviceId": "hubspot-template-2",
    "serviceName": "HubSpot Site",
    "version": 1,
    "logoUrl": "/modules/domain-connect/logos/hubspot.com.hubspot-template-2.png",
    "description": "Enables a domain to work with HubSpot",
    "variableDescription": "v1 is the customer id.",
    "records": [
        {
            "type": "CNAME",
            "host": "hs1",
            "pointsTo": "%cname0targetprefix%.dkim.hubspotemail.net",
            "ttl": "3600"
        },
        {
            "type": "CNAME",
            "host": "hs2",
            "pointsTo": "%cname1targetprefix%.dkim.hubspotemail.net",
            "ttl": "3600"
        },
        {
            "type": "TXT",
            "host": "smtpapi",
            "data": "%txtdata%",
            "ttl": "3600"
        }
    ]
}
