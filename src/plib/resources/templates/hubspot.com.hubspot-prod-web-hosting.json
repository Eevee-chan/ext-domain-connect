{
    "providerId": "hubspot.com",
    "providerName": "HubSpot",
    "serviceId": "hubspot-prod-web-hosting",
    "serviceName": "HubSpot Production Site",
    "version": 1,
    "logoUrl": "/modules/domain-connect/logos/hubspot.com.hubspot-prod-web-hosting.png",
    "description": "Enables a domain to work with the HubSpot CMS",
    "variableDescription": "DESTINATION: an account-specific subdomain.",
    "hostRequired": true,
    "records": [
        {
            "type": "CNAME",
            "host": "@",
            "pointsTo": "%DESTINATION%.sites.hubspot.net",
            "ttl": 600
        }
    ]
}
